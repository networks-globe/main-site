{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Layout/Layout.jsx", "components/Layout/Footer/Footer.tsx", "components/Layout/Header/Header.jsx", "store/action_creators/CourseAction.js", "store/constants/constants.js", "store/reducers/CourseReducer.js", "store/reducers/RootReducers.js", "store/sagas/CourseSaga.js", "store/sagas/RootSaga.js", "store/store.js", "pages/[slug].jsx", "pages/contact.jsx", "pages/index.jsx", "pages/about.jsx", "pages/_app.tsx"],
  "exclude": ["node_modules"]
}
